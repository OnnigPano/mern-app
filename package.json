    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
